{
    "name": "hexlet/code",
    "description": "project для вычисление отличий данных в двух файлах",
    "type": "project",
    "bin": ["bin/gendiff"],
    "license": "MIT",
    "authors": [
        {
            "name": "allomir",
            "email": "alomir@yandex.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hexlet\\Code\\": "src"
        },
        "files": [
            "src/Functions/Standard.php",
            "src/Functions/DataAggregate.php",
            "src/Functions/FS.php",
            "src/Functions/DataFormats.php",
            "src/gendiff.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Hexlet\\Code\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "webmozart/assert": "^1.11",
        "phpunit/phpunit": "^9.6"
    },
    "require": {
        "docopt/docopt": "^1.0"
    }
}

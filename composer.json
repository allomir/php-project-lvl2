{
    "name": "hexlet/code",
    "description": "project для вычисление отличий данных в двух файлах",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "allomir",
            "email": "alomir@yandex.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hexlet\\Code\\": "src/"
        },
        "files": [
            "src/Functions/Standard.php"
        ]

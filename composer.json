            "tests/Functions/StandardByWebmozartTest.php"
        ]
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "phpunit/phpunit": "^9.6",
        "webmozart/assert": "^1.11"
    }
}
